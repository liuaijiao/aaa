end type

event clicked;string ls_file,ls_filename
long ll_return1,ll_return
ll_return1 = getfilesavename("请选择要保存的文件类型：",ls_file,ls_filename,'DOC',"RTF File(*.rtf),*.rtf,TXT File(*.txt),*.txt,Doc File(*.doc;*.docx),*.doc;*.docx,HTML File(*.html),*.html,PDF File(*.pdf),*.pdf")

	if ll_return1 > 0 then
		if pos(ls_file,'.doc') > 0 then
				ll_return = rte_1.savedocument( ls_file,filetypedoc!,EncodingANSI!)
		elseif pos(ls_file,'.txt') > 0 then
			ll_return = rte_1.savedocument( ls_file,filetypetext!,EncodingANSI!)
		elseif pos(ls_file,'.rtf') > 0 then
			ll_return = rte_1.savedocument( ls_file,filetyperichtext!,EncodingANSI!)
		elseif pos(ls_file,'.html') > 0 then
			ll_return = rte_1.savedocument( ls_file,filetypehtml!,EncodingANSI!)
		else
			ll_return = rte_1.savedocument( ls_file,filetypepdf!,EncodingANSI!)
		end if
	end if


if ll_return > 0 then
	messagebox("Success","Save to "+ls_file+" Success")
end if




end event


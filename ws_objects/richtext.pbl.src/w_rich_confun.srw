event clicked;long ll_1,ll_2,ll_3,ll_4
band lb_band
string ls_band
string ls_str
lb_band = rte_1.position(ll_1,ll_2,ll_3,ll_4)
//ls_str = "the fromline is: " + string(ll_1) + "," + "the fromchar is: " + string(ll_2) + "," + "the toline is: " + string(ll_3) + "," + "the tochar is: " + string(ll_4)
ls_str = "rte_1.position(ll_1,ll_2,ll_3,ll_4)="+ string(ll_1) + "," + string(ll_2) + ","+ string(ll_3) + ","+ string(ll_4) 
choose case lb_band
	case header!
		ls_band = "header!" 
		messagebox("Band","rte_1.position(ll_1,ll_2,ll_3,ll_4)=" + ls_band)
		mle_1.text += ls_str + "~r~n"
	case detail!
		ls_band = "detail!" 
		messagebox("Band","rte_1.position(ll_1,ll_2,ll_3,ll_4)=" + ls_band)
		mle_1.text += ls_str + "~r~n"
	case footer!
		ls_band = "footer!" 
		messagebox("Band","rte_1.position(ll_1,ll_2,ll_3,ll_4)=" + ls_band)
		mle_1.text += ls_str + "~r~n"
	CASE ELSE
         ls_band = " No band"
	     messagebox("Band","rte_1.position(ll_1,ll_2,ll_3,ll_4)=" + ls_band)

end choose 
end event

